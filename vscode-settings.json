{
    "workbench.startupEditor": "welcomePage",
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "k"],
            "after": ["<Esc>"]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {   "before": ["H"],
            "after": ["^"]
        },
        {   "before": ["L"],
            "after": ["$"]
        }
    ],
    "workbench.colorTheme": "Visual Studio Light",
    "editor.fontSize": 13,
    "python.linting.flake8Enabled": true
}
